datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Place {
  id      Int    @id @default(autoincrement())
  title   String
  video   String
  country String
  region  String
  content String
  coordsX Float
  coordsY Float
}
